{
  "name": "with-vercel-website",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "ci": "pnpm build",
    "ci-migrate": "payload migrate && pnpm build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@payloadcms/admin-bar": "3.54.0",
    "@payloadcms/db-sqlite": "3.54.0",
    "@payloadcms/db-vercel-postgres": "3.54.0",
    "@payloadcms/live-preview-react": "3.54.0",
    "@payloadcms/next": "3.54.0",
    "@payloadcms/payload-cloud": "3.54.0",
    "@payloadcms/plugin-form-builder": "3.54.0",
    "@payloadcms/plugin-nested-docs": "3.54.0",
    "@payloadcms/plugin-redirects": "3.54.0",
    "@payloadcms/plugin-search": "3.54.0",
    "@payloadcms/plugin-seo": "3.54.0",
    "@payloadcms/richtext-lexical": "3.54.0",
    "@payloadcms/storage-vercel-blob": "3.54.0",
    "@payloadcms/translations": "3.54.0",
    "@payloadcms/ui": "3.54.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "better-auth": "1.3.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "error-ex": "1.3.2",
    "geist": "^1.5.1",
    "graphql": "^16.11.0",
    "libsql": "^0.5.22",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-sitemap": "^4.2.3",
    "payload": "3.54.0",
    "payload-auth": "^1.6.4",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "sharp": "0.34.3",
    "simple-swizzle": "0.2.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "1.54.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.8",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.5.2",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@tailwindcss/oxide",
      "browser-tabs-lock",
      "bufferutil",
      "core-js",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "utf-8-validate"
    ]
  }
}
